{
  "name": "browserslist",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#comment-line"
    },
    {
      "include": "#headers"
    },
    {
      "include": "#string"
    },
    {
      "include": "#browsers"
    },
    {
      "include": "#percents"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.browserslist",
          "match": "\\b(not|and|or|extends|,)\\b"
        }
      ]
    },
    "headers": {
      "patterns": [
        {
          "name": "header.control.browserslist",
          "match": "^(\\[\\w+\\])\\i"
        }
      ]
    },
    "comment-line": {
      "name": "comment.line.number-sign.browserslist",
      "begin": "#\\s",
      "end": "\\n"
    },
    "string": {
      "patterns": [
        {
          "name": "string.unquoted.browserslist",
          "begin": "^",
          "end": "$"
        }
      ]
    },
    "browsers": {
      "patterns": [
        {
          "name": "string.unquoted.browserslist",
          "match": "(?i)\\b(Android|BlackBerry|bb|Chrome|ChromeAndroid|Edge|Electron|Explorer|ie|FirefoxAndroid|and_ff|iOS|ios_saf|Baidu|Node|Opera|OperaMini|op_mini|OperaMobile|ExplorerMobile|ie_mob|Firefox|ff|op_mob|QQAndroid|and_qq|Safari|Samsung|UCAndroid|and_uc|kaios)\\b"
        }
      ]
    },
    "percents": {
      "patterns": [
        {
          "name": "constant.numeric.browserslist",
          "match": "\\d*0?\\.?\\d+%"
        }
      ]
    },
    "scopeName": "source.browserslist"
  }
}
